{
  "name": "app version in shops",
  "description": "app version in each shop",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "check_update_url": "https://raw.githubusercontent.com/Susreal/app-version-in-shops-crx/master/getCrxInfo.json",
  "gitHub_homePage": "https://github.com/Susreal/app-version-in-shops-crx/"
}
