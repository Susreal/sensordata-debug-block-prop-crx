{
  "name": "神策DEBUG助手",
  "description": "sensordata-debug-block-prop-crx",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "js/jquery-3.4.1.min.js",
      "js/background.js"
    ]
   },
  "content_scripts": [
    {
      "matches": ["https://*.baidu.com/*"], 
      "js": ["js/jquery-3.4.1.min.js","js/content_script.js"]
    }
  ]
}
